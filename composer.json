    "symfony/polyfill-php80": "^1.0",
    "symfony/polyfill-php81": "^1.0",
    "phpmailer/phpmailer": "^5.0",
    "phpsysinfo/phpsysinfo": "^3.0",
    "smarty/smarty": "^3.0",
    "softcommerce/jquery-flot": "^1.0",
    "welltime/phpagi": "2.20.1",
    "pear/soap": "^0.14",
    "ext-gettext": "*",
